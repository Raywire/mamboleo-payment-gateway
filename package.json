{
  "name": "mamboleo-payment-gateway",
  "version": "1.0.0",
  "description": "A payment gateway",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/server",
    "start:dev": "nodemon --exec babel-node -- ./src/server",
    "build": "rm -rf ./build && babel src -d ./build --copy-files",
    "lint": "eslint './src/**/*.js' --quiet",
    "create-env": "printenv > .env",
    "migrate": "sequelize db:migrate",
    "reset:db": "sequelize db:migrate:undo:all && npm run migrate && npm run seed",
    "seed": "sequelize db:seed:all",
    "test": "NODE_ENV=test npm run reset:db && NODE_ENV=test nyc mocha --require @babel/register --timeout 10000 src/tests/**/*.spec.js",
    "coveralls-coverage": "nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc report --reporter=lcov && npm run coveralls-coverage",
    "postinstall": "sequelize db:migrate",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "keywords": [
    "payment",
    "mpesa",
    "visa",
    "mastercard",
    "paypal"
  ],
  "author": "Mambo Leo Application Group",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "bcrypt": "^4.0.1",
    "celebrate": "^12.0.1",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.1",
    "pg": "^8.4.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.1.0",
    "snyk": "^1.685.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.11",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "sequelize-cli": "^5.5.1",
    "sinon": "^9.0.1",
    "sinon-chai": "^3.5.0",
    "sinon-express-mock": "^2.2.1"
  },
  "snyk": true
}
